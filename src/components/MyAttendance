import React from 'react';

function MyAttendance() {
  const attendance = [
    { date: '2025-07-01', course: 'Math', status: 'Present' },
    { date: '2025-07-02', course: 'CS', status: 'Absent' }
  ];

  return (
    <div>
      <h3>My Attendance</h3>
      <ul>
        {attendance.map((entry, idx) => (
          <li key={idx}>{entry.date}: {entry.course} - {entry.status}</li>
        ))}
      </ul>
    </div>
  );
}

export default MyAttendance;
